{
  "name": "url-shortening-app",
  "version": "1.0.1",
  "description": "URL Shortening Application",
  "main": "index.html",
  "scripts": {
    "start": "npm-run-all",
    "compile:sass": "node-sass sass/main.scss css/main.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/main.comp.css -o  css/main.prefix.css",
    "compress:css": "node-sass css/main.prefix.css css/main.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Divyajyoti1801/URL-Shortening-App.git"
  },
  "keywords": [
    "Html",
    "scss",
    "javascript",
    "API",
    "Layout",
    "Responsivness"
  ],
  "author": "Divyajyoti",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Divyajyoti1801/URL-Shortening-App/issues"
  },
  "homepage": "https://github.com/Divyajyoti1801/URL-Shortening-App#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concat": "^1.0.3",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "sass": "^1.57.1"
  }
}
